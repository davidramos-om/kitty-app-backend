{
    "version": 2,
    "builds": [
        {
            "src": "public/main.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "10mb",
                "includeFiles": [
                    "public/dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "public/dist/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}